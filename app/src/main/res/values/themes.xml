<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DateAppProject" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#5ECF2E</item>
        <item name="colorPrimaryVariant">#12C554</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#CAA31A</item>
        <item name="colorSecondaryVariant">#A6CC1F</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="datePickerTheme" parent="android:Theme.Material.Light.Dialog">
        <item name="android:colorAccent">#44CC12</item>
        ></style>

    <style name="Theme.DateAppProject.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.DateAppProject.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.DateAppProject.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>